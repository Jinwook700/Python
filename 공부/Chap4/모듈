import mod1
print(mod1.add(3, 4))
7

from mod1 import add
add(4, 2)
6

# mod2.py
def add(a, b): 
    return a+b

def sub(a, b): 
    return a-b

if __name__ == "__main__":
    print(add(1, 4))
    print(sub(4, 2))

